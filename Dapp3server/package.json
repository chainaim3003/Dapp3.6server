{
  "name": "zk-pret-http-server",
  "version": "1.0.0",
  "description": "HTTP Server for ZK-PRET tools - provides HTTP API access to ZK-PRET functionality",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "http-server": "npm run build && node dist/unified-server.js",
    "start": "npm run http-server",
    "dev": "tsx watch src/unified-server.ts",
    "sync-only": "npm run build && node dist/server.js",
    "async-only": "npm run build && node dist/async-server.js",
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "vercel-build": "npm run build",
    "vercel-dev": "npm run dev",
    "deploy": "vercel --prod",
    "deploy-preview": "vercel"
  },
  "keywords": [
    "zero-knowledge",
    "blockchain",
    "proofs",
    "regulatory",
    "enterprise",
    "verification",
    "http-api",
    "rest-api"
  ],
  "author": "ZK-PRET Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}